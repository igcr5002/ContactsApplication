<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane fx:controller="com.ignat.contacts.NewContactController"
            xmlns:fx="http://javafx.com/fxml"
            fx:id="dialogPaneId"
            prefHeight="200" prefWidth="350">
    <content>
        <GridPane hgap="15" vgap="15">
            <Label GridPane.columnIndex="0" GridPane.rowIndex="0" text="Nume: "/>
            <TextField fx:id="numeTextField" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
            <Label GridPane.rowIndex="1" GridPane.columnIndex="0" text="Prenume: "/>
            <TextField fx:id="prenumeTextField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <Label GridPane.rowIndex="2" GridPane.columnIndex="0" text="Numar de telefon: "/>
            <TextField fx:id="numarTelefonTextField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <Label GridPane.rowIndex="3" GridPane.columnIndex="0" text="E-mail: "/>
            <TextField fx:id="emailTextField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            </GridPane>
    </content>
</DialogPane>
